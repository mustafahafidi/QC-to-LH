{
  "name": "quickcheck-to-liquidhaskell",
  "version": "1.0.0",
  "main": "",
  "scripts": {
    "liquid-spy": "bash -c 'spy run -n \"yarn liquid $0 $1 $2 $3\" src/'",
    "liquid-spy-old": "sh -c 'spy run -n -g \"*$1\" \"yarn liquid $0\" src/'",
    "spy": "sh -c 'spy run -n \"yarn runghc $0\" src/'",
    "runghc": "stack exec -- runghc -isrc/ -i/home/mustafa/coding/liquidhaskell/include/ -i/home/mustafa/coding/liquidhaskell/src/ ",
    "ghci": "stack exec -- ghci -isrc/ -i/home/mustafa/coding/liquidhaskell/include/ -i/home/mustafa/coding/liquidhaskell/src/",
    "liquid": "stack --stack-yaml=/home/mustafa/coding/liquidhaskell/stack.yaml exec -- liquid -isrc/ "
  },
  "repository": "git@github.com:mustafahafidi/QC-to-LH.git",
  "author": "Mustafa <mustafahafidi96@gmail.com>",
  "license": "MIT"
}
