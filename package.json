{
  "name": "quickcheck-to-liquidhaskell",
  "version": "1.0.0",
  "main": "",
  "scripts": {
    "liquid-spy": "bash -c 'spy run -n \"yarn liquid $0 $1 $2 $3\" src/'",
    "liquid-th-spy": "bash -c 'spy run -n -g=$1 \"yarn liquid --ghc-option=\"-dth-dec-file\" $0 $2 $3\" src/'",
    "spy": "sh -c 'spy run -n \"yarn runghc $0\" src/'",
    "runghc": "stack exec -- runghc -isrc/",
    "ghci": "stack exec -- ghci -isrc/",
    "liquid": "stack exec -- liquid -isrc/ "
  },
  "repository": "git@github.com:mustafahafidi/QC-to-LH.git",
  "author": "Mustafa <mustafahafidi96@gmail.com>",
  "license": "MIT"
}
